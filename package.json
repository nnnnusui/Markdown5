{
	"publisher": "nnnnusui",
	"name": "markdown5",
	"displayName": "Markdown5",
	"description": "_",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown5"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "markdown5",
				"extensions": [
					".markdown5",
					".m5"
				]
			}
		],
		"semanticTokenTypes": [{
			"id": "sectionHeader",
			"description": "section header"
		}, {
			"id": "title",
			"description": "document title"
		}],
		"themes": [
			{
				"label": "Markdown5 semantics",
				"uiTheme": "vs-dark",
				"path": "./themes/semantics.json"
			}
		],
		"commands": [
			{
				"command": "markdown5.compile",
				"title": "Markdown5: Compile"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "markdown5-view",
					"title": "markdown5 explorer",
					"icon": "./resources/markdown5.svg"
				}
			]
		},
		"views": {
			"markdown5-view": [
				{
					"id": "markdown5",
					"name": "markdown5"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./",
		"lint:prettier": "prettier src --check",
		"lint:eslint": "eslint src",
		"lint": "run-p lint:*",
		"fix:prettier": "npm run lint:prettier -- --write",
		"fix:eslint": "npm run lint:eslint -- --fix",
		"fix": "run-s fix:*",
		"compile": "tsc -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"dev": "run-s fix compile",
		"demon": "nodemon -e ts --exec \"npm run dev\""
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"glob": "^7.1.6",
		"markdown5": "file:../parser",
		"mocha": "^8.2.1",
		"nodemon": "^2.0.7",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
