{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "markdown5",
    "patterns": [
        {
            "include": "#keywords"
        }
    ],
    "repository": {
         "keywords": {
             "patterns": [
                {
                    "name": "keyword.control",
                    "match": "#\\s"
                },
                {
                    "name": "keyword.blockPrefix",
                    "match": ">\\s"
                }
            ]
        }
    },
    "scopeName": "source.markdown5"
}